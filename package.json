  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy-config.json --open",
    "build": "ng build --aot --prod --output-hashing=none --build-optimizer=false",
    "build-app": "npm run build && compodoc -p src/tsconfig.app.json && gulp build:artifact && compodoc -p src/tsconfig.app.json",
    "build-package": "npm run build && ng-packagr -p ng-package.json && cd artifactory && npm pack && gulp format:package",
    "localServer": "node localServer/server.js",
    "test": "ng test",
    "test-coverage": "ng test --code-coverage --browsers PhantomJS --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json"
  },
